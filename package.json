{
  "name": "tsed-bun-example",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "bun run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "bun --watch src/index.ts",
    "compile": "bun build --target=bun ./src/index.ts --outfile=./dist/index.js",
    "compile:bin": "bun build --compile ./src/index.ts --outfile=bun-app"
  },
  "dependencies": {
    "@tsed/ajv": "^7.35.1",
    "@tsed/common": "^7.35.1",
    "@tsed/core": "^7.35.1",
    "@tsed/di": "^7.35.1",
    "@tsed/engines": "^7.35.1",
    "@tsed/exceptions": "^7.35.1",
    "@tsed/json-mapper": "^7.35.1",
    "@tsed/logger": "^6.6.2",
    "@tsed/logger-file": "^6.6.2",
    "@tsed/openspec": "^7.35.1",
    "@tsed/platform-cache": "^7.35.1",
    "@tsed/platform-exceptions": "^7.35.1",
    "@tsed/platform-express": "^7.35.1",
    "@tsed/platform-log-middleware": "^7.35.1",
    "@tsed/platform-middlewares": "^7.35.1",
    "@tsed/platform-params": "^7.35.1",
    "@tsed/platform-response-filter": "^7.35.1",
    "@tsed/platform-views": "^7.35.1",
    "@tsed/schema": "^7.35.1",
    "@tsed/components-scan": "7.35.1",
    "ajv": "^8.12.0",
    "barrelsby": "^2.8.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.3.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.6.0",
    "ts-node": "^10.9.1",
